{
    "name": "predicthq/address-formatter",
    "description": "Address formatter using country templates from https://github.com/OpenCageData/address-formatting",
    "keywords": ["predicthq","address"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PredictHQ",
            "homepage": "https://developer.predicthq.com"
        },
        {
            "name": "Robert Kern",
            "homepage": "http://www.robertkern.com",
            "role": "Developer"
        }
    ],
    "scripts": {
        "post-package-install": [
            "git clone https://github.com/predicthq/address-formatting.git",
            "cd address-formatting && git reset --hard f9e44ece2ac16c4a9872e76b31532b64f3f27667"
        ],
        "post-package-update": [
            "cd address-formatting && git reset --hard f9e44ece2ac16c4a9872e76b31532b64f3f27667"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/yaml": "~3.0",
        "mustache/mustache": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PredictHQ\\AddressFormatter\\": "src"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    }
}
